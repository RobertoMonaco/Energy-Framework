    <!-- http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#accept -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#accept">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Stakeholders"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Regulations"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Technologies"/>
        <rdfs:comment xml:lang="en">Stakeholder acceptance of new regulations and  technologies is one of the main factor to assess scalability and replicability of new projects in the energy domain. 
Stakeholders accept regulations and technologies</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#affect -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#affect">

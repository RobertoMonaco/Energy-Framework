<rdf:RDF xmlns="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#"
     xml:base="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"

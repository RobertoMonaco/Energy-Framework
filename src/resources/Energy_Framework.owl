        <rdfs:range rdf:resource="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Regulations"/>
        <rdfs:comment xml:lang="en">Policy makers enact new regulations</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#rule -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#rule">

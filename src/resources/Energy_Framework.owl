    <!-- http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Business -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Business">

        <rdfs:range rdf:resource="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Stakeholders"/>
        <rdfs:comment xml:lang="en">The type of market influences the decisions of stakeholders and their investments, it also influences the type of energy transmission network we would invest in.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#enact -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#enact">

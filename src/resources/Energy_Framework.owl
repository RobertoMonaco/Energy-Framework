        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A database can be of several types: Relational databases, Object-oriented databases, Distributed databases, Data warehouse, NoSQL database, Graphical databases, OLTP database, etc.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#EnergySector_type -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#EnergySector_type">

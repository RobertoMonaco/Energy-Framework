        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Each stakeholder has a different Business to take care, in a different Energy sector.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Country -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/robertomonaco/ontologies/2021/5/Energy_Framework#Country">
